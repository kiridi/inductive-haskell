rec reverse1 = lambda (xs, ys)
    if xs = nil
    then ys
    else reverse1(tail(xs), head(xs):ys);;

-- val id(x) @ a = x;;

val BK_tail(xs) = if xs = nil then nil else tail(xs);;

val BK_reverse(xs) = reverse1(xs, nil);;

